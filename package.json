{
  "name": "server",
  "version": "1.0.0",
  "description": "a node js chat server",
  "main": "index.js",
  "scripts": {
    "watch": "nodemon --exec babel-node lib/index.js",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "babel lib -d dist",
    "serve": "node dist/index.js",
    "prestart" : "npm run build",
    "start": "npm run serve",
    "heroku-postbuild": "npm install"
  },
  "author": "Nico",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "rimraf": "^3.0.2",
    "socket.io": "^2.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "nodemon": "^2.0.4"
  },
  "engines": {
    "node": "10.x"
  }
}
